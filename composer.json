{
    "name": "ebizmarts/magento2-mandrill",
    "description": "Connect Mandrill with Magento",
    "type": "magento2-module",
    "version": "4.0.0",
    "authors": [
        {
            "name": "Ebizmarts Corp",
            "email": "info@ebizmarts.com",
            "homepage": "http://ebizmarts.com"
        }
    ],
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "support": {
        "email": "info@ebizmarts.com",
        "wiki": "http://wiki.ebizmarts.com",
        "forum": "http://ebizmarts.com/forums/view/1"
    },
    "require" : {
        "mandrill/mandrill": "1.0.*",
        "magento/framework": "103.0.*"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Ebizmarts\\Mandrill\\": ""
        }
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
